{
  "name": "@flowui/vanilla-context-menu",
  "version": "0.15.0",
  "description": "This package offers an API that enables the addition of Custom Context Menus in a browser.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "unpkg": "umd/vanilla-context-menu.min.js",
  "types": "types/index.d.ts",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "node scripts/build.mjs",
    "build:ci": "pnpm build --version ignore",
    "format": "prettier -w -l \"**/*.{js,ts}\"",
    "prepare": "husky install"
  },
  "keywords": [
    "context menu",
    "browser context menu"
  ],
  "author": "Amed Moore <amed@amedmoore.com> (https://amedmoore.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.23.0",
    "@babel/preset-env": "7.22.20",
    "@babel/preset-typescript": "7.23.0",
    "@rollup/plugin-node-resolve": "15.2.1",
    "@rollup/plugin-terser": "0.4.3",
    "@rollup/plugin-typescript": "11.1.4",
    "app-root-path": "3.1.0",
    "chalk": "5.3.0",
    "compression-webpack-plugin": "10.0.0",
    "cross-env": "7.0.3",
    "execa": "8.0.1",
    "fs-extra": "11.1.1",
    "husky": "^8.0.0",
    "prettier": "3.0.3",
    "rollup": "3.29.4",
    "semver": "7.5.4",
    "ts-loader": "9.4.4",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4"
  }
}
